{
  "expo": {
    "name": "1日1回の無料占いアプリ",
    "slug": "lucky-day-v2",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2d2157"
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro",
      "output": "static",
      "meta": {
        "title": "【1日1回】今日の天体運勢",
        "description": "星の導きで今日の運勢を占います。恋愛運、仕事運、金運の中から選んで、あなただけの天体からのメッセージを受け取りましょう。",
        "og:type": "website",
        "og:title": "【1日1回】今日の天体運勢",
        "og:description": "星の導きで今日の運勢を占います。恋愛運、仕事運、金運の中から選んで、あなただけの天体からのメッセージを受け取りましょう。",
        "og:image": "https://images.unsplash.com/photo-1534447677768-be436bb09401?q=80&w=1200&auto=format&fit=crop"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.luckydayv2",
      "buildNumber": "1.0.1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "この機能を使用すると、プロフィール画像を選択したり、運勢の結果を保存したりできます。",
        "NSPhotoLibraryAddUsageDescription": "この機能を使用すると、運勢の結果をフォトライブラリに保存できます。",
        "NSCameraUsageDescription": "この機能を使用すると、プロフィール画像を撮影できます。",
        "UIBackgroundModes": [
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#2d2157"
      },
      "package": "com.astralfortune.app",
      "versionCode": 2,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "アプリがあなたの写真にアクセスすることを許可します。",
          "cameraPermission": "アプリがカメラを使用することを許可します。"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}